  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.4",
    "@types/ws": "0.0.38",
    "typescript": "^2.1.5",
    "tsify": "^3.0.0",
    "browserify": "^14.0.0"
  }
